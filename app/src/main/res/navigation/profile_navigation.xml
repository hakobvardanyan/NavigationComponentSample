<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/profile_navigation"
    app:startDestination="@id/profileFragment">

    <fragment
        android:id="@+id/profileFragment"
        android:name="com.hakvardanyan.navigationsample.main.profile.ProfileFragment"
        android:label="FragmentProfileChild1"
        tools:layout="@layout/fragment_profile">
        <action
            android:id="@+id/action_profileFragment_to_profileFragmentChild2"
            app:destination="@id/profileFragmentChild2" />
        <action
            android:id="@+id/action_profileFragment_to_notificationsFragment"
            app:destination="@id/notificationsFragment" />
        <action
            android:id="@+id/action_profileFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
        <action
            android:id="@+id/action_profileFragment_to_termsConditionsFragment"
            app:destination="@id/termsConditionsFragment" />
        <action
            android:id="@+id/action_profileFragment_to_privacyPolicyFragment"
            app:destination="@id/privacyPolicyFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFragmentChild2"
        android:name="com.hakvardanyan.navigationsample.main.profile.ProfileFragmentChild2"
        android:label="ProfileFragmentChild2"
        tools:layout="@layout/fragment_profile_child2">
        <action
            android:id="@+id/action_profileFragmentChild2_to_profileFragmentChild3"
            app:destination="@id/profileFragmentChild3" />
    </fragment>
    <fragment
        android:id="@+id/profileFragmentChild3"
        android:name="com.hakvardanyan.navigationsample.main.profile.ProfileFragmentChild3"
        android:label="ProfileFragmentChild3"
        tools:layout="@layout/fragment_profile_child3" />
    <fragment
        android:id="@+id/notificationsFragment"
        android:name="com.hakvardanyan.navigationsample.main.profile.NotificationsFragment"
        android:label="NotificationsFragment"
        tools:layout="@layout/fragment_notifications" >
        <action
            android:id="@+id/action_notificationsFragment_to_notificationDetailsFragment"
            app:destination="@id/notificationDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/notificationDetailsFragment"
        android:name="com.hakvardanyan.navigationsample.main.profile.NotificationDetailsFragment"
        android:label="NotificationDetailsFragment"
        tools:layout="@layout/fragment_notification_details" />
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.hakvardanyan.navigationsample.main.profile.SettingsFragment"
        android:label="SettingsFragment"
        tools:layout="@layout/fragment_settings" />
    <fragment
        android:id="@+id/termsConditionsFragment"
        android:name="com.hakvardanyan.navigationsample.main.profile.TermsConditionsFragment"
        android:label="TermsConditionsFragment"
        tools:layout="@layout/fragment_terms_conditions" />
    <fragment
        android:id="@+id/privacyPolicyFragment"
        android:name="com.hakvardanyan.navigationsample.main.profile.PrivacyPolicyFragment"
        android:label="PrivacyPolicyFragment"
        tools:layout="@layout/fragment_privacy_policy" />
</navigation>
